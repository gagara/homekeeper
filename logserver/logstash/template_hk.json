{
  "template" : "logstash_hk-*",
  "settings" : {
    "index.refresh_interval" : "5s"
  },
  "mappings" : {
    "_default_" : {
      "_all" : {"enabled" : true, "omit_norms" : true},
      "properties" : {
        "headers" : {
          "properties" : {
            "http_user_agent": { "type": "string", "index": "not_analyzed" }
          }
        },
        "message": { "type": "string", "index": "not_analyzed" },
        "prev_timestamp": { "type": "date" },
        "period": { "type": "long" },
        "csr_type": { "type": "string", "index": "not_analyzed" },
        "csr_state": { "type": "string", "index": "not_analyzed" },
        "sensor_type": { "type": "string", "index": "not_analyzed" },
        "name": { "type": "string", "index": "not_analyzed" },
        "state": { "type": "boolean" },
        "state_str": { "type": "string", "index": "not_analyzed" },
        "m": { "type": "string", "index": "not_analyzed" },
        "id": { "type": "byte" },
        "ns": { "type": "byte" },
        "ts": { "type": "long" },
        "ff": { "type": "byte" },
        "ft": { "type": "long" },
        "n" : {
          "properties" : {
            "id": { "type": "byte" },
            "ns": { "type": "byte" },
            "ts": { "type": "long" },
            "ff": { "type": "byte" },
            "ft": { "type": "long" }
          }
        },
        "n_id": { "type": "byte" },
        "n_ns": { "type": "byte" },
        "s" : {
          "properties" : {
            "id": { "type": "byte" },
            "v": { "type": "integer" },
            "ts": { "type": "long" },
            "cf": { "type": "float" }
          }
        },
        "s_v": { "type": "integer" },
        "ap": { "type": "string", "index": "not_analyzed" },
        "pw": { "type": "string", "index": "not_analyzed" },
        "sip": { "type": "ip" },
        "lip": { "type": "ip" },
        "sp": { "type": "integer" },
        "oc": { "type": "integer" }
      }
    }
  }
}

