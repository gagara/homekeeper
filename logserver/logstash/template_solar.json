{
  "template" : "logstash_solar-*",
  "settings" : {
    "index.refresh_interval" : "5s"
  },
  "mappings" : {
    "_default_" : {
      "_all" : {"enabled" : true, "omit_norms" : true},
      "properties" : {
        "headers" : {
          "properties" : {
            "http_user_agent": { "type": "keyword" }
          }
        }
      }
    }
  }
}

