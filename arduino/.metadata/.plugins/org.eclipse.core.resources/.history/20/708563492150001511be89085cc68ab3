#include "RadioSender1.h"

const uint64_t pipe = 0xE8E8F0F0E1LL;
const uint8_t len = 1;
uint8_t buff[len];

RF24 radio(9, 10);

void setup() {
    radio.begin();
    radio.openWritingPipe(pipe);
    buff[0] = HIGH;
}

void loop() {
    buff[0] ^= buff[0];
    radio.write(buff, len, 0);
    delay(1000);
}
